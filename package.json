{
	"author": {
		"name": "Oscar Palmér",
		"url": "https://oscarpalmer.se"
	},
	"description": "Signals and stuff…",
	"devDependencies": {
		"@biomejs/biome": "^1.9",
		"@rollup/plugin-node-resolve": "^16",
		"@rollup/plugin-typescript": "^12.1",
		"@types/node": "^22.10",
		"@vitest/coverage-istanbul": "^3",
		"dts-bundle-generator": "^9.5",
		"glob": "^11",
		"happy-dom": "^16.6",
		"tslib": "^2.8",
		"typescript": "^5.7",
		"vite": "^6",
		"vitest": "^3"
	},
	"exports": {
		".": {
			"import": {
				"types": "./types/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./types/index.d.ts",
				"default": "./dist/index.cjs"
			}
		}
	},
	"files": [
		"dist",
		"src",
		"types"
	],
	"keywords": [
		"signal",
		"signals",
		"reactive"
	],
	"license": "MIT",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"name": "@oscarpalmer/mora",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/oscarpalmer/mora.git"
	},
	"scripts": {
		"build": "npm run clean && npm run build:js && npm run rollup && npm run types",
		"build:js": "npx vite build",
		"clean": "rm -rf ./dist && rm -rf ./types && rm -f ./tsconfig.tsbuildinfo",
		"rollup": "npx rollup -c",
		"test": "npx vitest --coverage",
		"types": "npx tsc && npx dts-bundle-generator --config ./dts.config.cts",
		"watch": "npx vite build --watch"
	},
	"type": "module",
	"types": "./types/index.d.ts",
	"version": "0.1.0"
}
